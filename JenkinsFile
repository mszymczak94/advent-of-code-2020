pipeline {
	agent any
	stages {
		stage('Init') {
			steps {
				echo "Hey this is Michal from EY"
				echo "We are starting the testing"
			}
		}
		stage('Build') {
			steps {
				echo "Building Sample Maven project"
			}
		}
		stage('Deploy') {
			steps {
				echo "Deploying in Stagin Area"
			}
		}
		stage('Deploy Production') {
			steps {
				echo "Deploying in Production Area"
			}
		}
	}
}